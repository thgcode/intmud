int32 pontos

func cmd_atkenv
  pontos = arg1.pvida

func cmd_atacou
  refvar vida = arg0.pvida
  refvar max = arg0.pvidamax
  se !arg2.result && vida && vida < max
    pontos = intpos(pontos - arg1.pvida) / 2
    vida += pontos
    vida > max && (vida = max)
  fimse


