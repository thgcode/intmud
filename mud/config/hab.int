func cmd_atacar
# Checa se o alvo está cego
  se !(arg1.bitver & 1)
    $mens.p(arg0, arg1)
    $mens.mtodos1("$P lança um raio nos olhos $d $a, mas el$w está ceg$t!")
    ret 1
  fimse
# Checa se o alvo está usando algo nos olhos
  listaitem i
  epara i = arg1.dentro2.ini, i && i.obj.vestpos != 3, i.depois
  efim
  se i.obj
    $mens.p(arg0, arg1, i.obj)
    $mens.mtodos1("$P lança um raio nos olhos $d $a, mas $o desvia o raio.")
    ret 1
  fimse


